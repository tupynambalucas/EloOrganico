{
  "geminicodeassist.contextExclusionGitignore": false,
  "geminicodeassist.contextExclusionFile": ".gemini/.geminiignore",
  "geminicodeassist.chat.enableStreaming": true,
  "geminicodeassist.enableTelemetry": false,
  "geminicodeassist.chat.automaticScrolling": false,
  "geminicodeassist.inlineSuggestions.nextEditPredictions": true,
  "http.systemCertificatesNode": true,
  "gemini-cli.debug.logging.enabled": true,
  "geminicodeassist.inlineSuggestions.enableAuto": true,
  "geminicodeassist.inlineSuggestions.suggestionSpeed": "Fast",
  "google.gemini-cli-vscode-ide-companion": true,
  "geminicodeassist.languages": [
    "typescriptreact",
    "typescript",
    "html",
    "dockerfile",
    "dockercompose",
    "css",
    "json",
    "powershell",
    "markdown",
    "git-commit",
    "yaml",
    "plaintext"
  ],
  "general": {
    "disableAutoUpdate": false,
    "previewFeatures": true
  },
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github@latest"]
    }
  }
}
